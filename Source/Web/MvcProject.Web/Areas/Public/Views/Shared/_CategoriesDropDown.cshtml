@using MvcProject.Web.Areas.Public.ViewModels.Categories

@model List<CategoryForLayoutDropDown>

<ul class="dropdown-menu">
    @for (int i = 0; i < Model.Count(); i++)
    {
        <li>@Html.ActionLink(Model[i].Name, "CategoryOverView", "Categories", new { categoryId = Model[i].Id }, null)</li>
    }
</ul>