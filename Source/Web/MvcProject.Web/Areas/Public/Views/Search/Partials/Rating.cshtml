@model double?

@{
    var maxVote = 5;
    var htmlAttributes = (Dictionary<string, object>)ViewData["htmlAttributes"] ?? new Dictionary<string, object>();
}

@(Html.Infragistics()
    .RatingFor(m => m)
    .ID((string)ViewData["ratingId"])
    .VoteCount(maxVote)
    //.Value(Model.Value)
    .ValueAsPercent(false)
    //.Focusable(false)
    .Disabled(true)
    .PrecisionZeroVote(0.1)
    .Precision(RatingPrecision.Exact)
    .HtmlAttributes(htmlAttributes)
    .Render()
)