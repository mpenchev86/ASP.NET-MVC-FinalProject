@(Html.TabStripHelper(
    "tabStrip_#=Id#",
    0,
    items =>
    {
        items.Add().Text("Products").Content(@<text>@Html.Partial("_ProductsTab")</text>);
        items.Add().Text("SearchFilters").Content(@<text>@Html.Partial("_SearchFiltersTab")</text>);
        items.Add().Text("Keywords").Content(@<text>@Html.Partial("_KeywordsTab")</text>);
    })
    .ToClientTemplate()
    .ToMvcClientTemplate())
